<%- include ('../../partials/header.ejs')%>
<%- include ('../../partials/navbar.ejs')%>
<div class="container">
    <div class="card mt-4">
        <div class="card-header d-flex justify-content-between">
            <h2>Users</h2>
            <a href="/admin/users/create" class="btn btn-lg btn-primary">Create new User</a>
        </div>
        <div class="card-body">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                <% users.forEach((user)=>{ %>
                <tr>
                    <th><%= user.id %></th>
                    <th><%= user.email %></th>
                </tr>
                <%})%>
                </tbody>
            </table>
        </div>
    </div>
    
</div>
<%- include ('../../partials/footer.ejs')%>